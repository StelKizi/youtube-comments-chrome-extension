{
    "name": "Youtube Popup Comments",
    "description": "Pops up comments according to video time stamp",
    "version": "0.1.0",
    "manifest_version": 2,
    "icons": {
        "16": "./icons/icon16.png",
        "32": "./icons/icon32.png",
        "128": "./icons/icon128.png"
    },

    "background": {
        "scripts":["background.js"]
    },
    "options_page": "./options.html",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        
        "https://www.youtube.com/watch",
        "storage"
    ]
}